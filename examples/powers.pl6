say 2¹; # 2
say 2²; # 4
say 2³; # 8
say 2⁴; # 16
say 2⁵; # 32
say 2⁶; # 64
say 2⁷; # 128
say 2⁸; # 256
say 2⁹; # 512

say 10⁻¹; # 0.1
