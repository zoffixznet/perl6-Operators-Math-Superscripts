#!perl6

use v6;
use Test;
use lib 'lib';
use Operators::Math::Superscripts;

is 2⁰, 2**0, '2⁰';
is 2¹, 2**1, '2¹';
is 2², 2**2, '2²';
is 2³, 2**3, '2³';
is 2⁴, 2**4, '2⁴';
is 2⁵, 2**5, '2⁵';
is 2⁶, 2**6, '2⁶';
is 2⁷, 2**7, '2⁷';
is 2⁸, 2**8, '2⁸';
is 2⁹, 2**9, '2⁹';

is 10⁻¹, 10**-1, '10⁻¹';
is 10⁻², 10**-2, '10⁻²';
is 10⁻³, 10**-3, '10⁻³';
is 10⁻⁴, 10**-4, '10⁻⁴';
is 10⁻⁵, 10**-5, '10⁻⁵';
is 10⁻⁶, 10**-6, '10⁻⁶';
is 10⁻⁷, 10**-7, '10⁻⁷';
is 10⁻⁸, 10**-8, '10⁻⁸';
is 10⁻⁹, 10**-9, '10⁻⁹';

done-testing;
